// @test: Issue 10
// @typecheck
// ============================================================================

enum ReallyLongEnumName {
    Foo,
    Bar,
    Baz
}

enum AnotherReallyLongEnumName {
    Alpha = 4,
    Beta,
    Gamma
}

class Foo {
    prop x = ReallyLongEnumName.Foo;
    static prop y = AnotherReallyLongEnumName.Beta;
}


let f = new Foo();

assert.equal(f.x, 0);
assert.equal(f.x, ReallyLongEnumName.Foo);

assert.equal(Foo.y, 5);
assert.equal(Foo.y, AnotherReallyLongEnumName.Beta);

f.x = ReallyLongEnumName.Baz;
f.x = "oops"; // @type

Foo.y = AnotherReallyLongEnumName.Alpha;
Foo.y = 7; // @type

